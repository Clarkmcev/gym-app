<div class="container">

    <header class="row mt-3">

        <div class="text-center">
            <img src="/images/real-estate-background1.jpg" class="shadow-lg img-fluid mx-auto d-block" alt=" homepage">
        </div>
    </header>

    <div class="container px-3 shadow-lg bg-dark bg-gradient" id="featured-3">
        <div class="row g-6 row-cols-1 row-cols-lg-3">
            <div class="feature col text-center bg-dark bg-gradient text-white-50 p-3">

                <div
                    class="feature-icon rounded d-inline-flex align-items-center justify-content-center bg-primary bg-light text-white fs-2 mb-2">
                    <img class="rounded" style="max-height: 1em;" src="images/home-logo1.png">
                </div>
                <h4 style="color: #E1D89F ; font-size:larger ; ">our website</h4>
                <h6 class="text-white-50"> sdadsafddsafa
                </h6>
            </div>
            <div class="feature col text-center bg-dark bg-gradient text-white-50 p-3">
                <div
                    class="feature-icon rounded d-inline-flex align-items-center justify-content-center bg-primary bg-light text-white fs-2 mb-2">
                    <img class="rounded" style="max-height: 1em;" src="images/home-logo1.png">
                </div>
                <h4 style="color: #E1D89F ; font-size:larger ; ">Exercises</h4>
                <h6 class="text-white-50"> text

                </h6>
            </div>
            <div class="feature col text-center bg-dark bg-gradient text-white-50 p-3">
                <div
                    class="feature-icon rounded d-inline-flex align-items-center justify-content-center bg-primary bg-light text-white fs-2 mb-2">
                    <img class="rounded" style="max-height: 1em;" src="images/home-logo1.png">
                </div>
                <h4 style="color: #E1D89F ; font-size:larger ; ">And New Exercises</h4>
                <h6 class="text-white-50"> Text</h6>
            </div>
        </div>
    </div>

    <div>
        <div class="album pt-3 mx-5"></div>
        <h3>Our Properties</h3>
        <p class="text-secondary">SOME POPERTIES FROM OUR WEBSITE </p>
        <hr>

        <div class="container-fluid">
            <div class="row row-cols-1 row-cols-sm-3 row-cols-md-4 g-4 mb-5">

                {{#each foundOffers}}

                <div class="col">
                    <div class="card h-100 border-0 bg-dark bg-gradient text-light shadow-lg">
                        <img class="bd-placeholder-img card-img-top" width="100%" height="180" src="{{this.images}}"
                            role="img" aria-label="Placeholder: Thumbnail" preserveAspectRatio="xMidYMid slice"
                            focusable="false">
                        <rect width="100%" height="100%" fill="#55595c"></rect></img>
                        <div class="card-body">
                            <h6 style="color: #E1D89F; class=" card-text">{{this.name}}</h6>
                        </div>
                        <div class="text-center mb-3">
                            <a href="/offer/details/{{this._id}}" class=" btn
                                    btn-outline-" style="color: #E1D89F; role=" button">See
                                More</a>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>



</div>